import pandas as pd
import numpy as np

# To read the data in the csv file
data = pd.read_csv("s1.csv")
print(data, "\n")

# Making an array of all the attributes (all columns except last)
d = np.array(data)[:, :-1]
print("\nThe attributes are:\n", d)

# Segregating the target (last column)
target = np.array(data)[:, -1]
print("\nThe target is:\n", target)

# Training function to implement FIND-S algorithm
def train(c, t):
    # Initialize specific hypothesis to the first positive example
    for i, val in enumerate(t):
        if val == "Yes":
            specific_hypothesis = c[i].copy()
            break
    
    # For each positive example, generalize hypothesis
    for i, val in enumerate(c):
        if t[i] == "Yes":
            for x in range(len(specific_hypothesis)):
                if val[x] != specific_hypothesis[x]:
                    specific_hypothesis[x] = '?'
    return specific_hypothesis

# Obtaining the final hypothesis
final_hypothesis = train(d, target)
print("\nThe final hypothesis is:", final_hypothesis)




